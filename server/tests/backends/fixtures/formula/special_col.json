{
  "exclude": [
    "athena_pypika",
    "bigquery_pypika",
    "mysql_pypika",
    "postgres_pypika",
    "redshift_pypika",
    "snowflake",
    "snowflake_pypika"
  ],
  "step": {
    "pipeline": [
      {
        "name": "formula",
        "new_column": "foobar",
        "formula": "[column with spaces] + [an other column]"
      }
    ]
  },
  "input": {
    "schema": {
      "fields": [
        {
          "name": "NAME",
          "type": "string"
        },
        {
          "name": "column with spaces",
          "type": "integer"
        },
        {
          "name": "an other column",
          "type": "integer"
        }
      ],
      "pandas_version": "0.20.0"
    },
    "data": [
      {
        "NAME": "foo",
        "column with spaces": 42,
        "an other column": 100
      },
      {
        "NAME": "Azz",
        "column with spaces": 21,
        "an other column": 100
      }
    ]
  },
  "expected": {
    "schema": {
      "fields": [
        {
          "name": "NAME",
          "type": "string"
        },
        {
          "name": "column with spaces",
          "type": "number"
        },
        {
          "name": "an other column",
          "type": "integer"
        },
        {
          "name": "foobar",
          "type": "number"
        }
      ],
      "pandas_version": "0.20.0"
    },
    "data": [
      {
        "NAME": "foo",
        "column with spaces": 42,
        "an other column": 100,
        "foobar": 142
      },
      {
        "NAME": "Azz",
        "column with spaces": 21,
        "an other column": 100,
        "foobar": 121
      }
    ]
  }
}
