{
  "exclude": [
    "mongo",
    "mysql",
    "pandas",
    "postgres",
    "snowflake"
  ],
  "step": {
    "pipeline": [
      {
        "name": "aggregate",
        "on": [
          "beer_kind"
        ],
        "aggregations": [
          {
            "newcolumns": [
              "volume_ml_sum"
            ],
            "aggfunction": "sum",
            "columns": [
              "volume_ml"
            ]
          },
          {
            "newcolumns": [
              "first_name"
            ],
            "aggfunction": "first",
            "columns": [
              "name"
            ]
          }
        ]
      }
    ]
  },
  "expected": {
    "schema": {
      "fields": [
        {
          "name": "beer_kind",
          "type": "string"
        },
        {
          "name": "volume_ml_sum",
          "type": "number"
        },
        {
          "name": "first_name",
          "type": "string"
        }
      ],
      "pandas_version": "1.4.0"
    },
    "data": [
      {"beer_kind": "Belge blonde forte & Golden Ale", "volume_ml_sum": 330.0, "first_name": "Pauwel Kwak"},
      {"beer_kind": "Best-sellers", "volume_ml_sum": 330.0, "first_name": "Ardwen Blonde"},
      {"beer_kind": "Blanche & Weizen", "volume_ml_sum": 500.0, "first_name": "Weihenstephan Hefe Weizen Alcoholarm"},
      {"beer_kind": "Blonde", "volume_ml_sum": 910.0, "first_name": "Brasserie De Sutter Brin de Folie"},
      {"beer_kind": "India Pale Ale", "volume_ml_sum": 660.0, "first_name": "Bellfield Lawless Village IPA"},
      {"beer_kind": "Sans alcool", "volume_ml_sum": 330.0, "first_name": "Brewdog Nanny State Alcoholvrij"},
      {"beer_kind": "Triple", "volume_ml_sum": 330.0, "first_name": "Superstrong beer"}
    ]
  }
}
