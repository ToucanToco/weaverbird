{
  "exclude": [
    "athena",
    "mysql",
    "postgres",
    "postgres_pypika",
    "redshift",
    "snowflake"
  ],
  "step": {
    "pipeline": [
      {
        "name": "ifthenelse",
        "if": {
          "column": "NAME",
          "value": ".oo",
          "operator": "matches"
        },
        "newColumn": "COND",
        "then": "'tintin'",
        "else": "'glinglin'"
      }
    ]
  },
  "input": {
    "schema": {
      "fields": [
        {
          "name": "NAME",
          "type": "string"
        },
        {
          "name": "AGE",
          "type": "integer"
        },
        {
          "name": "SCORE",
          "type": "integer"
        }
      ],
      "pandas_version": "0.20.0"
    },
    "data": [
      {
        "NAME": "foo",
        "AGE": 42,
        "SCORE": 100
      },
      {
        "NAME": "bar",
        "AGE": 20,
        "SCORE": 200
      }
    ]
  },
  "expected": {
    "schema": {
      "fields": [
        {
          "name": "NAME",
          "type": "string"
        },
        {
          "name": "AGE",
          "type": "number"
        },
        {
          "name": "SCORE",
          "type": "integer"
        },
        {
          "name": "COND",
          "type": "string"
        }
      ],
      "pandas_version": "0.20.0"
    },
    "data": [
      {
        "NAME": "foo",
        "AGE": 42,
        "SCORE": 100,
        "COND": "tintin"
      },
      {
        "NAME": "bar",
        "AGE": 20,
        "SCORE": 200,
        "COND": "glinglin"
      }
    ]
  }
}