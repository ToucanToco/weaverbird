      {
        "name": "filter",
        "condition": {
          "and": [{
            "column": "CREATED",
            "operator": "from",
            "value": "date: 2021-11-01 12:34:56"
          }, {
            "column": "CREATED",
            "operator": "until",
            "value": "date: 2021-11-30 21:43:01"
          }]
        }
      }
    ]
  },
